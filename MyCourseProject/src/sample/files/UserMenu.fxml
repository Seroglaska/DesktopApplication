<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="404.0" prefWidth="606.0" style="-fx-background-color: #ADFF2F;" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.controllers.usercontrollers.UserController">
    <children>
        <AnchorPane layoutY="73.0" prefHeight="330.0" prefWidth="606.0" style="-fx-background-color: #008000;">
            <children>
                <Button fx:id="profileButton" layoutX="242.0" layoutY="35.0" mnemonicParsing="false" onAction="#clickProfileButton" prefHeight="46.0" prefWidth="122.0" text="Профиль">
                    <font>
                        <Font name="System Bold" size="12.0" />
                    </font>
                </Button>
                <Button fx:id="furnitureButton" layoutX="242.0" layoutY="118.0" mnemonicParsing="false" onAction="#clickFurnitureButton" prefHeight="46.0" prefWidth="122.0" text="Мебель">
