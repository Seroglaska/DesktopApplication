                        <Font name="System Bold" size="14.0" />
                    </font>
                </Button>
                <Button fx:id="orderButton" layoutX="242.0" layoutY="208.0" mnemonicParsing="false" onAction="#clickOrderButton" prefHeight="46.0" prefWidth="122.0" text="Заказы">
                    <font>
                        <Font name="System Bold" size="14.0" />
                    </font>
                </Button>
            </children>
        </AnchorPane>
        <Button fx:id="exitButton" layoutX="457.0" layoutY="14.0" mnemonicParsing="false" onAction="#clickExitButton" prefHeight="34.0" prefWidth="122.0" text="Выйти">
            <font>
                <Font name="System Bold" size="14.0" />
            </font>
        </Button>
    </children>
</AnchorPane>
